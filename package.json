{
  "name": "todorant-backend",
  "version": "1.0.0",
  "description": "Todorant backend",
  "main": "dist/server.js",
  "repository": "https://github.com/backmeupplz/todorant-backend",
  "author": "backmeupplz <backmeupplz@gmail.com>",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "distribute": "yarn build-ts && node dist/server.js",
    "develop": "concurrently -i -k -p \"[{name}]\" -n \"Node, TypeScript\" -c \"yellow.bold,cyan.bold\" \"yarn watch-js\" \"yarn watch-ts\"",
    "build-ts": "tsc --skipLibCheck || true",
    "watch-ts": "tsc -w --skipLibCheck",
    "watch-js": "nodemon dist/server.js -d 1",
    "upload-translations": "node scripts/upload.js",
    "download-translations": "node scripts/download.js && yarn prettier --single-quote --no-semi --write ./locales/errors.js",
    "kill-server": "lsof -ti:1337 | xargs kill && lsof -ti:3000 | xargs kill",
    "test": "yarn jest --forceExit",
    "pretty": "prettier --check src",
    "lint": "yarn pretty && eslint --max-warnings 0 --ext ts,tsx,json src"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/http-graceful-shutdown": "^2.3.0",
